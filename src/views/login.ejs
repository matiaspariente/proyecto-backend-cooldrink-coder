<%- include('./partials/head'); %>
<body>
    <div class="container my-5">
        <div>
            <h1>Login</h1>
            <br>
            <form class="form m-5" method="post" autocomplete="off" action="/login" >
                <div class="form-group m-2">
                    <input type="text" name="username" id="username" placeholder="email" class="form-control" required>
                </div>
                <div class="form-group m-2">
                    <input type="password" name="password" id="password" placeholder="password" class="form-control" required>
                </div>
                <div class="form-group m-2">
                    <input type="submit" value="Login" class="btn btn-success my-3">
                </div>
            </form>
            <hr>
            <div class="text-left m-5">
                <button class="btn btn-primary" onclick="location.href = '/registro'">Go to registro</button>
            </div>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
</body>
</html>